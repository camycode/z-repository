{
    "name": "zhaohehe/zrepository",
    "description": "Laravel Repositories",
    "keywords": ["laravel", "repository", "repositories", "eloquent", "database", "transformer"],
    "license": "MIT",
    "authors": [
        {
            "name": "zhaohehe",
            "email": "zhaohehedola@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "league/fractal": "^0.13",
        "illuminate/support": "^5.2",
        "illuminate/database": "^5.2",
        "illuminate/filesystem": "^5.2",
        "illuminate/console": "^5.2",
        "illuminate/events": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Zhaohehe\\": "src/Zhaohehe/",
            "Test\\": "test"
        }
    },
   "extra": {
        "laravel": {
            "providers": [
                "Zhaohehe\\Repositories\\Providers\\RepositoryProvider"
            ]
        }
    },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
    "minimum-stability": "dev"
}
